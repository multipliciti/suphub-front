{
	"printWidth": 85,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"jsxBracketSameLine": false,
	"overrides": [{ "files": "*.json", "options": { "printWidth": 200 } }],
	"spaces": { "tabSize": 2 }
}
